= #partial('layouts/section', locals: { title: 'Some Title' }, template_engine: :haml)
.ui.section.import
  %table.ui.very.basic.celled.table
    %tbody
      %h3.ui.header.centered
        Existing Lists
      - [1,2,3].each do |listing|
        %tr
          %td
            Add Data
          %td
            %h4.inline
              = listing.to_s
            %span.right-float
              %b
                Rows
          %td.centered
            %a
              %i.cog.large.blue.icon

